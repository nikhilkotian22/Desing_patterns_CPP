#ifndef LIGHTOFFCOMMAND_H_
#define LIGHTOFFCOMMAND_H_

#include "Command.h"
#include<iostream>
using namespace std;

class LightOffCommand: public Command {
public:
	LightOffCommand()
	{

	}
	~LightOffCommand()
	{

	}

	virtual void execute()
	{
		cout<<"Lights off"<<endl;
	}
};

#endif /* LIGHTOFFCOMMAND_H_ */
