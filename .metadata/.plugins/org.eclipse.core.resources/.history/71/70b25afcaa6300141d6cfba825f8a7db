#ifndef MYITERATOR_H_
#define MYITERATOR_H_

#include "MenuItems.h"
/*
 * Java style iterator because C++ iterator is cumbersome
 *
 */
class MyMenuIterator {
public:
	MyMenuIterator(){}
	~MyMenuIterator(){}
	virtual bool hasNext()= 0;
	virtual MenuItems next() = 0;
};

#endif /* MYITERATOR_H_ */
