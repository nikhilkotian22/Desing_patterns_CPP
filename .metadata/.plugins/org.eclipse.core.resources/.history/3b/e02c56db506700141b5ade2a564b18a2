#ifndef NOQUARTER_H_
#define NOQUARTER_H_

#include "GumBallState.h"

class NoQuarter: public GumBallState {
public:
	NoQuarter()
	{

	}
	~NoQuarter(){}

	virtual GumBallState* insertCoin()
	{
		return this;
	}
	virtual GumBallState* ejectCoin() { return this;}
	virtual GumBallState* turnCrank() {}
};

#endif /* NOQUARTER_H_ */
