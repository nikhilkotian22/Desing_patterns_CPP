#ifndef MYITERATOR_H_
#define MYITERATOR_H_
/*
 * Java style iterator because C++ iterator is cumbersome
 *
 */
class MyIterator {
public:
	MyIterator(){}
	~MyIterator(){}
	virtual bool hasNext()= 0;

};

#endif /* MYITERATOR_H_ */
